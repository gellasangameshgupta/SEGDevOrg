<aura:component implements="force:hasRecordId,force:hasSObjectName,force:lightningQuickAction" controller="MyObjController" >
    <aura:attribute name="recordId" type="String"/>
    <aura:attribute name="sObjectName" type="String"/>
    <aura:attribute name="whatId" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" access="public"/>
    <lightning:button variant="brand" label="Create Subtask" onclick="{!c.createRecordEvent}" />
</aura:component>